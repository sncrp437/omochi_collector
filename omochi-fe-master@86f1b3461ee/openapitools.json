{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.0.1",
    "generators": {
      "v1": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/src/generated/api",
        "inputSpec": "http://localhost:8000/api/schema/",
        "additionalProperties": {
          "npmName": "@api/client",
          "npmVersion": "1.0.0",
          "supportsES6": "true",
          "withInterfaces": true
        },
        "skipValidateSpec": true,
        "typeMappings": {
          "PageDto": "any",
          "UserDto": "any"
        }
      }
    }
  }
}